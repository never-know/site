							<input id="regname" type="text" class="regname" name="regname" tabindex="1" autocomplete="off" value="ybcoming" placeholder="邮箱/用户名/已验证手机">
							<i class="icon_reg iconfont">&#xe63b;</i>
							<span id="regname_error"></span>
						</div>
						<div class="login-pwd">
							<label for="regpwd"><b class="red">*</b>请设置密码：</label>
							<input type="password" id="regpwd" name="regpwd" class="regpwd" tabindex="2" autocomplete="off" placeholder="密码">
							<i class="icon_reg iconfont">&#xe63a;</i>
							<span class="capslock" style="display: none;"><b></b>大小写锁定已打开</span>
						</div>
						<div class="login-pwd">
							<label for="regpwd1"><b class="red">*</b>请确认密码：</label>
							<input type="password" id="regpwd1" name="regpwd1" class="regpwd" tabindex="2" autocomplete="off" placeholder="密码">
